{
  "extends": "next/core-web-vitals",
  "rules": {
    "@next/next/no-img-element": "off",
    "no-unused-vars": ["warn", { 
      "argsIgnorePattern": "^_",
      "varsIgnorePattern": "^_"
    }],
    "no-undef": "error"
  },
  "ignorePatterns": [
    ".next/**",
    "out/**",
    "node_modules/**",
    "dist/**",
    ".s/**"
  ]
} 